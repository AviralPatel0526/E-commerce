{
    "rewrites": [
      {
        "source": "/api/v1/(.*)",
        "destination": "https://e-commerce-arui.onrender.com/api/v1/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  